{% from "macros/case-study-header.njk" import case_study_header %}
{% from "macros/form.njk" import talk_form with context %}
{% from "macros/case-study-nav.njk" import case_study_nav %}

{% extends "base.njk" %}

{% block page_styles %}
<link rel="stylesheet" href="./assets/css/pages/case-study-base.css">
<link rel="stylesheet" href="./assets/css/pages/case-study-wantedly.css">
{% endblock %}

{% block meta_tag %}
<title>Wantedly Brand Communications – The Universe Fantastic: Creative Brand Agency | Singapore</title>
<meta property="og:url" content="http://theuniversefantastic.co/case-studies/wantedly/">
<meta property="og:type" content="website">
<meta property="og:title" content="Wantedly Brand Communications – The Universe Fantastic: Creative Brand Agency | Singapore">
<meta property="og:description" content="Find out how we communicated Wantedly's brand position in Singapore and helped them revolutionize talent recruitment.">
<meta property="og:image" content="http://theuniversefantastic.co/assets/images/tuf-facebook-share.jpg">
<meta name="description" content="Find out how we communicated Wantedly's brand position in Singapore and helped them revolutionize talent recruitment.">
{% endblock %}

{% block base_tag %}
<base href="/">
{% endblock %}

{% block content %}
{{
  case_study_header(
    title='Wantedly<span class="tuf-header-br">Brand Communications</span>',
    tags=['Branding', 'Advertising']
  )
}}

<section class="hero">
  <img
    class="hero__image"
    draggable="false"
    src="/assets/img/case-studies/wantedly/hero__background.jpg" alt="creative illustrations for brand development project done for Wantedly Singapore">
    <img
      class="hero__trophy"
      draggable="false"
      src="/assets/img/case-studies/wantedly/hero__trophy.png">
    <img
      class="hero__man"
      draggable="false"
      src="/assets/img/case-studies/wantedly/hero__man.png">
</section>

<section class="intro">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column intro__left">
        <img
          class="intro__image"
          draggable="false"
          src="/assets/img/case-studies/wantedly/intro__image.png"
          alt="brand positioning for social recruitment agency Wantedly Singapore"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column cs-container__column--flex cs-container__column--center intro__content">
        <p data-aos="fade-up">
          Wantedly is the largest social-recruitment jobs platform in the world. Headquartered in Japan and having recently expanded to Singapore, Wantedly needed an effective way to communicate their brand’s core propositions to the press for their launch.
        </p>
      </div>
    </div>
  </div>
</section>

<section class="crunch">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column">
        <h2 data-aos="fade-up">
          With only 2 weeks to work, we didn’t have much time.
        </h2>
        <p data-aos="fade-up">
          We worked closely with the Wantedly team to craft their communications, and then created several unique brand characters to populate their brand universe.
        </p>
      </div>
      <div class="col-md cs-container__column cs-container__column--flex cs-container__column--center crunch__left">
        <img
          class="crunch__image"
          draggable="false"
          src="/assets/img/case-studies/wantedly/crunch__timer.png"
          alt="countdown to brand launch for social recruitment agency Wantedly Singapore"
          data-aos="fade-up" />
      </div>
    </div>
  </div>
</section>

<section class="tuf">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column cs-container__column--flex cs-container__column--center">
        <img
          class="tuf__image"
          draggable="false"
          src="/assets/img/case-studies/wantedly/tuf__highlight.png"
          alt="brand communications for Wantedly Singapore"
          data-aos="fade-up" />
      </div>
    </div>
  </div>
</section>

<section class="characters">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column characters__content">
        <p
          data-aos="fade-up">
          Character Design & Illustration
        </p>
      </div>
    </div>
  </div>
  <div class="cs-container characters__wrapper">
    <div class="cs-container__row">
      <div class="col-md cs-container__column">
        <img
          draggable="false"
          src="/assets/img/case-studies/wantedly/character-1.png"
          alt="Character design and creative illustration for Wantedly Singapore's brand launch event"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column">
        <img
          draggable="false"
          src="/assets/img/case-studies/wantedly/character-2.png"
          alt="Character design and creative illustration for Wantedly Singapore's brand launch event"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column">
        <img
          draggable="false"
          src="/assets/img/case-studies/wantedly/character-3.png"
          alt="Character design and creative illustration for Wantedly Singapore's brand launch event"
          data-aos="fade-up" />
      </div>
    </div>
    <img
      class="characters__bottom"
      draggable="false"
      src="/assets/img/case-studies/wantedly/character-bottom.png"
      alt="Character design and creative illustration for Wantedly Singapore's brand launch event"
      data-aos="fade-up" />
  </div>
</section>

<section class="fb">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column fb__title">
        <p
          data-aos="fade-up">
          Facebook Ads & Event Posters
        </p>
      </div>
    </div>
    <div class="cs-container__row">
      <div class="col-md cs-container__column fb__content">
        <img
          class="fb__cards fb__cards--left"
          draggable="false"
          src="/assets/img/case-studies/wantedly/brand__left.png"
          alt="brand advertising poster for Wantedly Singapore, a social talent recruitment agency" data-aos="fade-up" />
        <img class="fb__cards fb__cards--right"
          draggable="false"
          src="/assets/img/case-studies/wantedly/connect__left.png"
          alt="brand communications asset for Wantedly Singapore, a social talent recruitment agency" data-aos="fade-up" />
        <img
          class="fb__icons fb__icons--left"
          draggable="false"
          src="/assets/img/case-studies/wantedly/connect.png" alt="brand communications asset for Wantedly Singapore, a social talent recruitment agency" data-aos="fade-up">
        <img
          class="fb__cards"
          draggable="false"
          src="/assets/img/case-studies/wantedly/discover__left.png"
          alt="brand advertising for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column fb__content fb__content--right">
        <img
          class="fb__cards"
          draggable="false"
          src="/assets/img/case-studies/wantedly/brand__right.png"
          alt="brand ad for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
        <img
          class="fb__icons fb__icons--left"
          draggable="false"
          src="/assets/img/case-studies/wantedly/brand.png"
          alt="brand advertising for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
        <img
          class="fb__cards fb__cards--right"
          draggable="false"
          src="/assets/img/case-studies/wantedly/connect__right.png"
          alt="brand advertising for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
        <img
          class="fb__cards"
          draggable="false"
          src="/assets/img/case-studies/wantedly/discover__right.png"
          alt="brand creative for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
        <img
          class="fb__icons fb__icons--left"
          draggable="false"
          src="/assets/img/case-studies/wantedly/discover.png" alt="brand creative for Wantedly Singapore, a social talent recruitment agency"
          data-aos="fade-up" />
      </div>
    </div>
  </div>
  <div class="fb__support-wrapper">
    <img
      class="fb__support"
      draggable="false"
      src="/assets/img/case-studies/wantedly/fb__support--char.jpg"
      alt="character and spatial design for Wantedly Singapore's brand launch event" />
      <img
        class="fb__support"
        draggable="false"
        src="/assets/img/case-studies/wantedly/fb__support--poster.jpg"
        alt="targeted brand communications placed at Wantedly Singapore's brand launch event" />
      <img
        class="fb__support"
        draggable="false"
        src="/assets/img/case-studies/wantedly/fb__support--placard.jpg"
        alt="photo wall for Wantedlly Singapore's brand launch to increase brand engagement" />
  </div>
</section>

<section class="collaterals">
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column collaterals__title">
        <p
          data-aos="fade-up">
          Design Collaterals
        </p>
      </div>
    </div>
    <div class="cs-container__row">
      <div class="col-md cs-container__column collaterals__poster">
        <img
          draggable="false"
          src="/assets/img/case-studies/wantedly/collaterals--steve.png"
          alt="Steve jobs do what you love quote used to create brand engagement content for Wantedly Singapore"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column collaterals__poster">
        <img
          draggable="false"
          src="/assets/img/case-studies/wantedly/collaterals--love.png"
          alt="Rumi love what you do quote used to create brand engagement content for Wantedly Singapore" data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column collaterals__poster">
        <img
          draggable="false" src="/assets/img/case-studies/wantedly/collaterals--joy.png"
          alt="brand content created for Wantedly Singapore brand launch"
          data-aos="fade-up" />
      </div>
    </div>
    <div class="cs-container__row">
      <div class="col-md cs-container__column collaterals__bottom  collaterals__hand-wrapper">
        <img
          class="collaterals__hand"
          draggable="false"
          src="/assets/img/case-studies/wantedly/collaterals__hand.png"
          alt="Flyer and brochure design for Wantedly Singapore branding"
          data-aos="fade-up" />
      </div>
      <div class="col-md cs-container__column collaterals__bottom ">
        <img
          class="collaterals__pullup"
          draggable="false"
          src="/assets/img/case-studies/wantedly/collaterals__pullup.png"
          alt="creative standee design for Wantedly Singapore branding"
          data-aos="fade-up" />
      </div>
    </div>
  </div>
</section>

<section class="result">
  <div class="result__bg"></div>
  <div class="cs-container">
    <div class="cs-container__row">
      <div class="col-md cs-container__column">
        <h2
          data-aos="fade-up">The result?
        </h2>
        <p data-aos="fade-up">
          An effective and memorable launch that communicated the core benefits of Wantedly's products and platforms on event day.
        </p>
      </div>
      <div class="col-md cs-container__column">
      </div>
    </div>
  </div>
  <img
    class="result__wall"
    draggable="false"
    src="/assets/img/case-studies/wantedly/result__wall.jpg"
    alt="spatial design for Wantedly Singapore brand launch event" />
</section>

{{
  case_study_nav(
    prev_link="/case-studies/rate",
    next_link="/case-studies/starbucks-bogo"
  )
}}

<!-- talk to us section -->
{{talk_form()}}
<!-- end of talk to us section -->
{% endblock %}

{% block page_script %}
<script src="./assets/js/case-study-base.min.js"></script>
{% endblock %}